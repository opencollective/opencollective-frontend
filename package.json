  "scripts": {
    "start": "node dist/server",
    "dev": "babel-node src/server",
    "build": "npm run build:updates && npm run build:clean && npm run build:next && npm run build:server",
    "build:clean": "rm -rf dist src/.next",
    "build:langs": "rm -rf dist/messages && babel src/**/*.js src/**/**/*.js > /dev/null && babel-node scripts/translate.js",
    "build:updates": "npm --prefix node_modules/cloudflare-ip run update-list",
    "build:next": "./scripts/build_next.sh",
    "build:server": "./scripts/build_server.sh",
    "test": "npm run test:jest && npm run test:e2e",
    "test:jest": "TZ=UTC jest src/*",
    "test:e2e": "TZ=UTC ./scripts/run_e2e_tests.sh",
    "test:update": "npm run test:jest -- --updateSnapshot",
    "deploy:production": "./scripts/pre-deploy.sh production && git push production master",
    "deploy:staging": "./scripts/pre-deploy.sh staging && git push -f staging master",
    "git:clean": "./scripts/git_clean.sh",
    "lint": "eslint \"cypress/**/*.js\" \"src/**/*.js\" \"test/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "pretest": "npm run lint:quiet && npm run prettier:check",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "cypress": "TZ=UTC cypress run",
    "cypress:open": "TZ=UTC cypress open",
    "prettier": "prettier \"*.@(js|json|md)\" \"cypress/**/*.@(js|json|md)\" \"src/**/*.@(js|json|md)\" \"test/**/*.@(js|json|md)\"",
    "optimize-png": "npm install --no-save imagemin imagemin-optipng && babel-node ./scripts/optimize-png",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --list-different",
    "graphql:get-schema": "npx graphql-cli get-schema -p opencollective -e dev && prettier src/graphql/schema.graphql --write",
    "styleguide:dev": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "styleguide:deploy": "npm run styleguide:build && npx now styleguide && npx now alias -A styleguide/now.json"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/node": "7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-intl": "^3.5.0",
    "babel-plugin-react-remove-properties": "0.3.0",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "commitizen": "^3.1.1",
    "cypress": "^3.3.2",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.0.1",
    "eslint-config-opencollective": "^2.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-styled-components": "^7.0.0-beta.1",
    "jsdom": "^15.1.1",
    "lint-staged": "^9.0.2",
    "mkdirp": "^0.5.1",
    "prettier": "1.18.2",
    "raf": "^3.4.1",
    "raw-loader": "^3.0.0",
    "react-styleguidist": "9.0.4",
    "react-test-renderer": "^16.8.6",
    "sinon": "^7.3.2",
    "url-loader": "^2.0.1",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/mocks/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      ".history"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,graphql}": [
      "prettier --write",
      "git add"
    ]
  }
}
