{
  "env": "NODE_ENV",
  "database": {
    "database": "PG_DATABASE",
    "username": "PG_USERNAME",
    "password": "PG_PASSWORD",
    "options": {
      "host": "PG_HOST"
    }
  },
  "keys": {
    "opencollective": {
      "secret": "JWT_SECRET",
      "resetPasswordSecret": "RESET_PASSWORD_SECRET"
    }
  },
  "stripe": {
    "secret": "STRIPE_SECRET",
    "key": "STRIPE_KEY",
    "clientId": "STRIPE_CLIENT_ID"
  },
  "aws": {
    "s3": {
      "key": "AWS_KEY",
      "secret": "AWS_SECRET",
      "bucket": "AWS_S3_BUCKET"
    }
  },
  "paypal": {
    "classic": {
      "email": "PAYPAL_EMAIL",
      "appId": "PAYPAL_APP_ID",
      "userId": "PAYPAL_USER_ID",
      "password": "PAYPAL_PASSWORD",
      "signature": "PAYPAL_SIGNATURE"
    }
  },
  "mailgun": {
    "user": "MAILGUN_USER",
    "password": "MAILGUN_PASSWORD"
  },
  "host": {
    "api": "API_URL",
    "webapp": "WEBAPP_URL",
    "publicpage": "PUBLIC_PAGE"
  }
}
